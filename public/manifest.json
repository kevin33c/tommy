{
  "name": "Tommy Sales Assitant",
  "description": "Tool to assist sales teams in usage of CRM",
  "version": "0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Use Tommy"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-3.6.1.min.js","contentScript.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "cookies"
  ],
  "host_permissions": [
    "https://www.linkedin.com/"
  ],
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  }
}